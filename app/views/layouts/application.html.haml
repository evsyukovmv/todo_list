!!!5
%html
  %head
    %title
      = title
    = csrf_meta_tag
    = render 'layouts/stylesheets'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    .container
      = render 'layouts/header'
      = yield
      - if content_for? :javascript
        %script{:type => "text/javascript"}
          = yield :javascript
      = debug(params) if Rails.env.development?