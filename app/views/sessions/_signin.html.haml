#signin
  = form_for(:session, url: sessions_path, class: 'well') do |f|
    .field
      = f.label :email
      = f.text_field :email
    .field
      = f.label :password
      = f.password_field :password
    .actions
      = f.submit "Sign in", class: 'btn-success'
      = link_to "Sign up now!", signup_path, :class => "signup"